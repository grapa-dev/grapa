/* Exception debug test */

"=== EXCEPTION DEBUG TEST ===\n".echo();

/* Test 1: Try to catch any exception */
"Test 1: Try to catch any exception\n".echo();
try {
    result = "hello".grep("hello");
    "Result: " + result.str() + "\n".echo();
} catch (e) {
    "Exception caught: " + e.str() + "\n".echo();
}
"---\n".echo();

/* Test 2: Try with invalid regex */
"Test 2: Try with invalid regex\n".echo();
try {
    result = "hello".grep("(");
    "Result: " + result.str() + "\n".echo();
} catch (e) {
    "Exception caught: " + e.str() + "\n".echo();
}
"---\n".echo();

/* Test 3: Try with empty pattern */
"Test 3: Try with empty pattern\n".echo();
try {
    result = "hello".grep("");
    "Result: " + result.str() + "\n".echo();
} catch (e) {
    "Exception caught: " + e.str() + "\n".echo();
}
"---\n".echo();

"=== EXCEPTION DEBUG COMPLETED ===\n".echo(); 