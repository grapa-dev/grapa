/* Logical NOT Operator (!) Test */

"=== LOGICAL NOT OPERATOR (!) TEST ===\n".echo();

/* Test 1: !BOOL */
"Test 1: !BOOL\n".echo();
result1 = !true;
if (result1) {
    "!true is TRUE\n".echo();
} else {
    "!true is FALSE\n".echo();
}
result1b = !false;
if (result1b) {
    "!false is TRUE\n".echo();
} else {
    "!false is FALSE\n".echo();
}

