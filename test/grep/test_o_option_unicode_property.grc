/* Unicode Property Extraction with 'o' Option Test */

"=== UNICODE PROPERTY 'o' OPTION TEST ===\n".echo();

input = "Hello 世界 123";
pattern = "\\p{L}+";

result = input.grep(pattern, "o");
("Result: " + result.str() + "\n").echo();
"Expected: [\"Hello\", \"世界\"]\n".echo();

"=== TEST COMPLETE ===\n".echo(); 