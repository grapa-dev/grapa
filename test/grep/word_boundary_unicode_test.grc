/* Word boundary Unicode test */
"=== WORD BOUNDARY UNICODE TEST ===\n".echo();

"--- ASCII Test ---\n".echo();
result1 = "hello hello hello".grep("hello", "ow");
"ASCII Result: ".echo(); result1.echo(); "\n".echo();

"--- Unicode Test ---\n".echo();
result2 = "hello 世界 hello".grep("hello", "ow");
"Unicode Result: ".echo(); result2.echo(); "\n".echo();

result3 = "世界 hello 世界".grep("世界", "ow");
"Unicode Pattern Result: ".echo(); result3.echo(); "\n".echo();

"Expected: Should find non-overlapping matches in both cases\n".echo();
"=== TEST COMPLETE ===\n".echo(); 