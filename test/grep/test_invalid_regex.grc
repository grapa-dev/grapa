/* Test invalid regex patterns */

"=== INVALID REGEX TEST ===\n".echo();

/* Test 1: Unmatched opening parenthesis */
"Test 1: Unmatched opening parenthesis\n".echo();
result1 = "foo".grep("(", "o");
("Result: " + result1.str() + "\n").echo();
("Expected: [] (empty array)\n").echo();
"---\n".echo();

/* Test 2: Unmatched closing parenthesis */
"Test 2: Unmatched closing parenthesis\n".echo();
result2 = "foo".grep(")", "o");
("Result: " + result2.str() + "\n").echo();
("Expected: [] (empty array)\n").echo();
"---\n".echo();

/* Test 3: Invalid quantifier */
"Test 3: Invalid quantifier\n".echo();
result3 = "foo".grep("a{", "o");
("Result: " + result3.str() + "\n").echo();
("Expected: [] (empty array)\n").echo();
"---\n".echo();

"=== TEST COMPLETE ===\n".echo(); 