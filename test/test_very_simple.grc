/* Very simple test */

"=== VERY SIMPLE TEST ===\n".echo();

/* Test 1: Just echo a string */
"Test 1: Just echo a string\n".echo();
"hello".echo();
"---\n".echo();

/* Test 2: Simple grep */
"Test 2: Simple grep\n".echo();
result = "hello".grep("hello");
"Result: " + result.str() + "\n".echo();
"---\n".echo();

/* Test 3: Check result type */
"Test 3: Check result type\n".echo();
"Result type: " + result.type() + "\n".echo();
"---\n".echo();

/* Test 4: Check if result is empty */
"Test 4: Check if result is empty\n".echo();
if (result.empty()) {
    "Result is empty\n".echo();
} else {
    "Result is not empty\n".echo();
}
"---\n".echo();

"=== VERY SIMPLE TEST COMPLETED ===\n".echo(); 